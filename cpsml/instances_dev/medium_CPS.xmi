<?xml version="1.0" encoding="UTF-8"?>
<cpsml:CPS xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:cpsml="http://big.tuwien.ac.at/cpsml"
    xsi:schemaLocation="http://big.tuwien.ac.at/cpsml ../model/cpsml.ecore"
    name="medium">
  <environment>
    <node>
      <publication
          messageBroker="//@messageBroker.0"/>
      <connectionModule
          xsi:type="cpsml:WirelessModule"/>
      <position/>
      <component
          xsi:type="cpsml:Sensor"/>
    </node>
    <node>
      <subscription/>
      <publication
          messageBroker="//@messageBroker.0"/>
      <connectionModule
          xsi:type="cpsml:WiredModule"/>
      <connectionModule
          xsi:type="cpsml:WirelessModule"/>
      <position/>
      <component
          xsi:type="cpsml:Sensor"/>
      <component
          xsi:type="cpsml:Actuator"/>
    </node>
  </environment>
  <environment>
    <node>
      <subscription
          messageBroker="//@messageBroker.0"/>
      <connectionModule
          xsi:type="cpsml:WirelessModule"/>
      <position/>
      <component
          xsi:type="cpsml:Actuator"/>
    </node>
  </environment>
  <messageBroker>
    <connectionModule
        xsi:type="cpsml:WirelessModule"/>
    <connectionModule
        xsi:type="cpsml:WiredModule"/>
    <position/>
  </messageBroker>
  <controller>
    <subscription
        messageBroker="//@messageBroker.0">
      <topics>sub</topics>
    </subscription>
    <publication
        messageBroker="//@messageBroker.0">
      <topics>pub</topics>
    </publication>
  </controller>
</cpsml:CPS>
